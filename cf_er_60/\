#include <bits/stdc++.h>

const int MAXN = 1e5 + 1;
const double EPS = 1e9;

int n;
double arr [MAXN];

double f (double a, double b, double c) {
  if (a < (b / (c + 1))
}

int main () {
  scanf ("%d", &n);
  for (int i = 0; i < n; i++)
    scanf ("%lf", arr + i);


  double best = arr [0], sum = arr [0], count = 1;
  for (int i = 1; i < n; i++) {
    sum = f (arr [i], sum + arr [i], cnt);
    best = std::max (best,sum);
  }
  return 0;
}
